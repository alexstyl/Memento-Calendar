<FindBugsFilter>

  <Match>
    <Or>
      <Class name="~.*\.R\$.*" />
      <Class name="~.*\.R\$.*" />
      <Class name="~.*Test" />
      <Class name="~.*Test\$.*" />
      <Class name="~.*\.Manifest\$.*" />
